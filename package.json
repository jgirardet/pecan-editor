{
  "name": "pecan-editor",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "mocha": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha src",
    "mocha-watch": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha src --watch",
    "cyp": "cypress open-ct -b firefox",
    "cyp-run": "cypress run-ct",
    "test-all": "npm-run-all -p cyp-run mocha"
  },
  "dependencies": {
    "@udecode/slate-plugins": "^1.0.0-alpha.25",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "slate": "^0.62.0",
    "slate-history": "^0.62.0",
    "slate-hyperscript": "^0.62.0",
    "slate-react": "^0.62.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "chai": "^4.3.4",
    "cypress": "^7.0.0",
    "@cypress/react": "^5.4.2",
    "@cypress/vite-dev-server": "^1.2.6",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2",
    "vite": "^2.2.3"
  }
}
